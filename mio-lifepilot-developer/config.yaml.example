# Mio-Lifepilot Developer - Configuration
# Kopiere diese Datei zu config.yaml und füge deine API-Keys ein

# API Keys für den Runden Tisch
api_keys:
  # OpenAI API (GPT)
  openai: "your_openai_key_here"
  
  # Anthropic API (Claude)
  anthropic: "your_anthropic_key_here"
  
  # Google AI (Gemini)
  google: "your_google_key_here"
  
  # xAI API (Grok)
  xai: "your_xai_key_here"

# GitHub Integration (optional)
github:
  token: "your_github_token_here"
  username: "your_username"

# Ausgabe-Einstellungen
output:
  directory: "./generated_code"
  format: "python"  # python, javascript, typescript, etc.

# Dashboard-Einstellungen (für Streamlit)
dashboard:
  enabled: false
  port: 8501
  host: "localhost"
